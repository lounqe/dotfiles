!-- #Xft
!-- -------------------------------------------------------------------------
	Xft.dpi       : 96
	Xft.antialias : true
	Xft.rgba      : rgb
	Xft.hinting   : true
	Xft.hintstyle : hintslight
!-- -------------------------------------------------------------------------

!-- #Colors
!-- -------------------------------------------------------------------------
	*.font              : xft:ohsnap:pixelsize=14
	!*foreground         : #FFFFFF
	!*cursorColor        : #e1e1e1
	!*underlineColor     : #ffffff
	*highlightColor     : #666666
	*highlightTextColor : #ffffff
	

! Zenburn

*foreground:   #e1e1e1
*background:   #2a2e35
! black + red
*color0:      #1E2320
*color1:      #705050
! green + yellow
*color2:      #60b48a
*color3:      #dfaf8f
! blue + purple
*color4:      #506070
*color5:      #dc8cc3
! cyan + white
*color6:      #8cd0d3
*color7:      #dcdccc
! bright-black + bright-red
*color8:      #709080
*color9:      #dca3a3
! bright-green + bright-yellow
*color10:     #c3bf9f
*color11:     #f0dfaf
! bright-blue + bright-purple
*color12:     #94bff3
*color13:     #ec93d3
! bright-cyan + bright-white
*color14:     #93e0e3
*color15:     #ffffff


!-- #Settings
!-- -------------------------------------------------------------------------
	!URxvt.background        : [95]#121212
	URxvt.depth             : 32
	URxvt*.transparent     : false
	
	URxvt*.shading         : 30
	URxvt.scrollBar         : false
	URxvt.scrollBar_right   : true
	URxvt.scrollstyle       : plain
	URxvt.scrollColor       : #ffffff
	URxvt.cursorUnderline   : false
	URxvt.cursorBlink       : true
	!URxvt.cursorColor       : #CEC9B2
	URxvt*borderless        : true
	URxvt*geometry          : 160x40
	URxvt*buffered          : true
	URxvt*scrollWithBuffer  : true
	URxvt*scrollTtyKeypress : true
	URxvt.internalBorder    : 5
	URxvt.scrollTtyOutput   : false
	URxvt.pointerBlank      : true
	URxvt.underlineURLs     : true
!-- -------------------------------------------------------------------------
