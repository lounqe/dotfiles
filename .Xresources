! Disable annoying ISO keycap mode pop up:
URxvt.iso14755: false
URxvt.iso14755_52: false

!Fonts
#define fonts \
    xft:hack:regular:pixelsize=10:antialias=true:hinting=true, \
    

URxvt.font: fonts
URxvt.letterSpace: -1
URxvt.boldFont:

URxvt.intensityStyles: false

!Say no to scrollbars
URxvt*scrollBar:false

! Size of my terminal
URxvt.geometry:75x20
URxvt.imLocale:en_US.UTF8

! Amount that I can scroll up
URxvt.saveLines: 12000

! Underline URLs
URxvt.underlineURLs:true

! Url-Select:
URxvt.perl-ext-common: ...,url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/google-chrome-stable

! Add color to my man pages:
!URxvt.colorIT: #87af5f
!URxvt.colorBD: #969694
!URxvt.colorUL:
